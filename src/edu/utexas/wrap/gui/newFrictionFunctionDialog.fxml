<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" prefHeight="150.0" prefWidth="100.0" spacing="5.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.utexas.wrap.gui.NewFrictionFunctionController">
   <children>
      <HBox disable="true" prefWidth="200.0" spacing="10.0">
         <children>
            <RadioButton mnemonicParsing="false" selected="true" text="Discretized" />
            <RadioButton mnemonicParsing="false" text="Gamma" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <VBox>
         <children>
            <Label text="Friction Function ID:" />
            <HBox spacing="10.0">
               <children>
                  <TextField id="skimSourcePath" fx:id="frictionFunctionID" minWidth="-Infinity" prefHeight="25.0" prefWidth="250.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox>
         <children>
            <Label text="Source file:" />
            <HBox spacing="10.0">
               <children>
                  <TextField id="skimSourcePath" fx:id="frictionFunctionSourceURI" disable="true" editable="false" minWidth="-Infinity" onAction="#changeFrictionFunctionSourceURI" prefHeight="25.0" prefWidth="250.0" HBox.hgrow="ALWAYS" />
                  <Button id="skimSourceBrowse" fx:id="frictionFunctionSourceBrowse" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#browseFrictionFunction" prefWidth="70.0" text="Browse..." />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
